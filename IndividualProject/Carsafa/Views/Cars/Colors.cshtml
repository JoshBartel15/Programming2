@model IEnumerable<string>

@{
    ViewData["Title"] = "Colors";
}

<h1>Colors</h1>


<table class="table">
    <thead>
        <tr>
            <th>
                Vin
            </th>
            <th>
                Year
            </th>
            <th>
                Model
            </th>
            <th>
                Color
            </th>
            <th>
                Type
            </th>
            <th>
                Make
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                <a href="Cars/Index?color=@item">@item</a>
            </td>
        </tr>
}
    </tbody>
</table>
